	Select 6 as order, iif(isnull(lib.kood),space(20),left(alltrim(lib.kood),3)) as grupp3, space(254) as gr3Nimi,;
		lib1.kood, lib1.nimetus,;
		iif(isnull(qryKuuAruanne.summa),000000000,qryKuuAruanne.summa) as kuu ;
