	Select order, grupp2, iif(isnull(lib.nimetus), space(254), lib.nimetus) as gr2Nimi, sum(kuu) as summa ;
		from qryKulud left outer join comObjektRemote lib on alltrim(qryKulud.grupp2) == alltrim(lib.kood);
