	Select order, grupp1, iif(isnull(lib.nimetus), space(254), lib.nimetus) as gr1Nimi, sum(kuu) as summa ;
		from qryKulud left outer join comObjektRemote lib on alltrim(qryKulud.grupp1) == alltrim(lib.kood);
