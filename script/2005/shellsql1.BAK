Function uuenda
	Lparameters lcUuenda
	LOCAL lcString
	If Empty(lcUuenda) Or !File(lcUuenda) Or Empty(gnHandle)
		Return 0
	Endif
	CREATE CURSOR v_sql (sql m)
	APPEND BLANK
	APPEND memo sql FROM (lcUuenda)
	lcString = v_sql.sql
	IF EMPTY(lcString)
		RETURN 0
	ELSE
		Return SQLEXEC(gnHandle,lcString)
	ENDIF
	
Endfunc

