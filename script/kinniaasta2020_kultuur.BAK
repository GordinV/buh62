		Select qryImportLog
		Locate For old_id = qrySaldoKreedit_.asutusId
		If !Found()
			lcString = 'SELECT * from asutus WHERE id = ' + STR(qrySaldoKreedit_.asutusId)
			lError = sqlexec(gnHandle2009 ,lcString, 'qryAsutus')
			If lError < 1
